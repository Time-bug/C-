#define  _CRT_SECURE_NO_WARNINGS

#include <stdio.h>

int main()
{
	int day = 0;
	scanf("%d", &day);
	switch (day)
	{
	case 1:
		printf("周一\n");
		break;
	case 2:
		printf("周2\n");
		break;
	case 3:
		printf("周3\n");
		break;
	case 4:
		printf("周4\n");
		break;
	case 5:
		printf("周5\n");
		break;
	case 6:
		printf("周6\n");
		break;
	case 7:
		printf("周7\n");
		break;
	}
	return 0;
}
